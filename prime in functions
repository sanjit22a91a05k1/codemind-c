// without argument and without return function
# include<stdio.h>
void prime();
int main()
{
	prime();
}
void  prime()
{
	int n, i, count=0;
	scanf("%d",&n);
	for(i=1; i<=n; i++)
	{
		if(n%i==0)
		{
			count=count+1;
		}
	}
	if(count==2)
	{
		printf("Prime");
	}
	else 
	{
		printf("Not prime");
	}
}
