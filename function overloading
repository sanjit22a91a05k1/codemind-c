//function overloading
#include<iostream>
using namespace std;
class box
{
	public:
	void area(float r)
	{
		cout<<"area of circle is "<<3.14*r*r<<endl;
	}
	void area(int side)
	{
		cout<<"area of square is "<<side*side<<endl;
	}
	void area(int l, int b)
	{
		cout<<"area of rectangle is "<<l*b<<endl;
	}
};
int main()
{
	box v;
	v.area(6.6f);
	v.area(5);
	v.area(10,20);
}
