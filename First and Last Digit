# include<stdio.h>
# include<math.h>
int main()
{
    int n,r,rem,s,a;
    scanf("%d",&n);
    for(int i=1; i<=n; i++)
    {
        int x;
        scanf("%d",&x);
        int c=0;
        int t=x;
        s=x%10;
        while(x!=0)
        {
            r=x%10;
            c++;
            x=x/10;
        }
         a=pow(10,c-1);
         rem=t/a;
      printf("%d \n",rem+s);

    }
    
}
