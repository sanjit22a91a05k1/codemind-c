#include <stdio.h>

int main(){
	int N = 0,c=0;
	scanf("%d", &N);
	
	long data[N];
	for(int i=0; i<N; i++)
	{
	    scanf("%ld", &data[i]);
		}
	for(int i=0; i<N; i++)
	{
		if(data[N-1]%10==0 )
		{
			c=1;
		}
	}
	if(c==1)
	printf("Yes");
	else 
	printf("No");
    // write your code here
    // ans =  

}
