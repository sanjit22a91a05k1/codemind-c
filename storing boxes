#include <bits/stdc++.h>
using namespace std;

int main() {

  int length = 11;
  int width = 13;
  cout << length * width << endl;
  cout << 2 * (length + width) << endl;
  width = 15; //Replace the _ with the correct value.
  cout << length * width << endl;
  cout << 2 * (length + width);
  
  return 0;
}
