// string palindrome without handiling function
# include<stdio.h>
# include<string.h>
int main()
{
	 char s1[50];
	 scanf("%s",s1);
	 int i,l=0,c=0;
	 for( i=0; s1[i]!='\0';i++)
	 {
	    l++;
	 }
	 for(int i=0,j=l-1; i<j; i++,j--)
	 {
	 	if(s1[i]!=s1[j])
	 	{
	 		c++;
		 }
	 }
	 if(c==0)
	 {
	 	printf("Palindrome");
	 }
	 else 
	 {
	 	printf("Not Palindrome ");
	 }
	 
   
}
